{
    "name": "sven/:package",
    "description": "",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Luijten",
            "email": "sven@luijten.co",
            "homepage": "https://svenluijten.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.11"
    },
    "autoload": {
        "psr-4": {
            "Sven\\:namespace\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sven\\:namespace\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sven\\:namespace\\ServiceProvider"
            ],
            "aliases": {
                ":namespace": "Sven\\:namespace\\Facades\\:namespace"
            }
        }
    },
    "scripts": {
        "check": [
            "@test",
            "@analyse"
        ],
        "test": "phpunit",
        "analyse": "phpstan analyse src/ tests/"
    }
}
